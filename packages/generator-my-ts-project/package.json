{
  "name": "@manuth/generator-my-ts-project",
  "version": "0.0.0",
  "description": "Generates TypeScript-Projects with custom additions",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "keywords": [
    "yo",
    "yeoman-generator",
    "Generator",
    "Custom",
    "manuth",
    "TypeScript"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/manuth/TSGeneratorGenerator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/TSGeneratorGenerator.git",
    "directory": "packages/generator-my-ts-project"
  },
  "bugs": {
    "url": "https://github.com/manuth/TSGeneratorGenerator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "rebuild": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "clean": "npm run build -- --clean && rimraf ./lib",
    "lint-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint": "npm run lint-base -- --ext .js,.jsx,.ts,.tsx ./src .eslintrc.js",
    "lint-compact": "npm run lint -- -f compact || exit 0",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "@manuth/extended-yo-generator": "^7.3.2",
    "@manuth/generator-ts-project": "^1.4.7",
    "chalk": "^4.1.0",
    "dedent": "^0.7.0",
    "detect-newline": "^3.1.0",
    "eol": "^0.9.1",
    "fs-extra": "^9.0.1",
    "temp-filesystem": "^1.1.3",
    "yaml": "^1.10.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^2.1.2",
    "@manuth/extended-yo-generator-test": "^7.3.2",
    "@manuth/tsconfig": "^1.2.2",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.1",
    "@types/yaml": "^1.9.7",
    "@types/yosay": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.3.0",
    "mocha": "^8.1.2",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "typescript-eslint-plugin": "^1.2.1",
    "yo": "^3.1.1"
  }
}
