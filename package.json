{
  "name": "@manuth/generator-ts-project",
  "version": "1.4.7",
  "description": "A Set of Generators for Creating Projects Written in TypeScript",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "yo",
    "yoeoman-generator",
    "Generator",
    "TypeScript"
  ],
  "homepage": "https://github.com/manuth/TSGeneratorGenerator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/TSGeneratorGenerator.git"
  },
  "bugs": {
    "url": "https://github.com/manuth/TSGeneratorGenerator/issues"
  },
  "scripts": {
    "build": "tsc -p .",
    "rebuild": "npm run clean && npm run build",
    "watch": "tsc -p . --watch",
    "clean": "rimraf ./lib",
    "lint-code": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./src",
    "lint-code-compact": "npm run lint-code -- -f compact || exit 0",
    "lint-docs": "markdownlint ./templates/**/*.md",
    "lint": "npm run lint-code && npm run lint-docs",
    "lint-compact": "npm run lint-code-compact && npm run lint-docs || exit 0",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "@manuth/extended-yo-generator": "^4.2.0",
    "@manuth/package-json-editor": "^1.4.1",
    "@types/vscode": "^1.47.0",
    "chalk": "^4.1.0",
    "comment-json": "^3.0.2",
    "dedent": "^0.7.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "parse-pkg-name": "^2.0.0",
    "temp-filesystem": "^1.1.3",
    "validate-npm-package-name": "^3.0.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^2.1.2",
    "@manuth/extended-yo-generator-test": "^4.2.0",
    "@manuth/tsconfig": "^1.2.2",
    "@types/comment-json": "^1.1.1",
    "@types/dedent": "^0.7.0",
    "@types/eslint": "^7.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.22",
    "@types/npm-which": "^3.0.0",
    "@types/validate-npm-package-name": "^3.0.0",
    "@types/yosay": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^29.1.4",
    "markdownlint-cli": "^0.23.2",
    "mocha": "^8.0.1",
    "npm-which": "^3.0.1",
    "random-js": "^2.1.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "typescript-eslint-plugin": "^1.2.1",
    "upath": "^1.2.0",
    "yo": "^3.1.1"
  }
}
